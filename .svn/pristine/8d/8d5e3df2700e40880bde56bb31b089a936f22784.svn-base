.row
 .col-lg-3.col-md-3
 .col-lg-6.col-md-6
  form(ng-submit="editarEntrega()")
   fieldset.form-group
    legend Editar Entregas
   .form-group
    label Fecha:
    input.form-control(type='text', ng-model="editEntrega.fechaEntrega" required)
    label Comentarios:
    input.form-control(type='text', ng-model="editEntrega.comentarios")
   button.btn.btn-primary() Enviar
   a.btn.btn-info(href='#/entregas/', role="button") Volver