form(name="formAlta",ng-submit="submit()")
		|Arquitecturas
		select(ng-model="alta.arquitectura",ng-options="item.nombre for item in arquitecturas")
		|Items
		select(ng-model="alta.item",ng-options="item.nombre for item in items")
		|Complejidades
		select(ng-model="alta.complejidad", ng-options="item.descripcion for item in complejidades")
		|Tamanios
		select(ng-model="alta.tamanio", ng-options="item.descripcion for item in tamanios")
		