.row
 .col-lg-3.col-md-3
 .col-lg-6.col-md-6
  form
   fieldset.form-group
     legend Alta de una nueva Entrega
    .form-group
     label Nombre Proyecto:
     input.form-control(name="nombre",type='text', ng-model="altaEntregas.proyecto.nombre")
     label Fecha Entrega:
     input.form-control(name="nombre",type='text', ng-model="altaEntregas.fechaEntrega", required)
     label Comentarios:
     input.form-control(name="nombre",type='text', ng-model="altaEntregas.comentarios")
      
   button.btn.btn-primary(ng-click="alta()") Enviar
   a.btn.btn-info(href='#/entregas/', role="button") Volver